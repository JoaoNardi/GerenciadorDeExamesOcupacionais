<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<AnchorPane prefHeight="400.0" prefWidth="420.0" xmlns="http://javafx.com/javafx/23.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.joaonardi.gerenciadorocupacional.controller.SetorController">
    <children>
        <Button fx:id="btnSalvar" layoutX="118.0" layoutY="346.0" mnemonicParsing="false" onAction="#handleSalvarSetor"
                text="Salvar"/>
        <Button fx:id="btnCancelar" layoutX="245.0" layoutY="346.0" mnemonicParsing="false"
                onAction="#handleCancelarSetor" text="Cancelar"/>
        <Label layoutX="14.0" layoutY="9.0" text="Setor">
            <font>
                <Font size="18.0"/>
            </font>
        </Label>
        <Label layoutX="106.0" layoutY="68.0" text="Area"/>
        <TextField fx:id="inputArea" layoutX="106.0" layoutY="92.0" prefHeight="25.0" prefWidth="165.0"
                   promptText="Exemplo: Escritorio"/>
    </children>
</AnchorPane>
