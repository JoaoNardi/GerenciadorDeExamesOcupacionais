<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="406.0" prefWidth="570.0" xmlns="http://javafx.com/javafx/11.0.14-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.joaonardi.gerenciadorocupacional.controller.TipoExameController">
    <children>
        <Button fx:id="btnSalvar" layoutX="170.0" layoutY="363.0" mnemonicParsing="false" onAction="#handleSalvarExame" text="Salvar" />
        <Button fx:id="btnCancelar" layoutX="332.0" layoutY="363.0" mnemonicParsing="false" onAction="#handleCancelarExame" text="Cancelar" />
        <Label layoutX="102.0" layoutY="14.0" text="Tipo Exame">
            <font>
                <Font size="18.0" />
            </font>
        </Label>
        <Label layoutX="207.0" layoutY="85.0" text="Complete os dados">
            <font>
                <Font size="14.0" />
            </font>
        </Label>
        <Label layoutX="147.0" layoutY="118.0" text="Nome:" />
        <TextField fx:id="inputNome" layoutX="198.0" layoutY="112.0" prefHeight="25.0" prefWidth="165.0" promptText="Tipo de Exame" />
        <Label layoutX="110.0" layoutY="149.0" text="Periodicidade:" />
        <ChoiceBox fx:id="inputPeriodicidade" layoutX="199.0" layoutY="143.0" prefHeight="25.0" prefWidth="165.0" />
      <ToggleButton layoutX="58.0" layoutY="178.0" mnemonicParsing="false" onAction="#handleAdicionarCondicoes" text="Adicionar Condições" />
        <ScrollPane fx:id="scrollPaneCondicoes" layoutX="35.0" layoutY="212.0" prefHeight="142.0" prefWidth="500.0">
            <content>
        <VBox fx:id="containerCondicoes" layoutX="15.0" layoutY="215.0" prefHeight="0.0" prefWidth="479.0" spacing="10.0" />
            </content>
        </ScrollPane>
    </children>
</AnchorPane>
